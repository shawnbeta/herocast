{{=<% %>=}}
<!-- Generated Code: mustache_templates/ui/navBar.mustache -->
<%# navBar %>
    <div class="swNavBar ">

    <div class="swContainer clearfix">
        <div class="col-xs-2 menuToggle smallOnly">
            <a href="#"> <i class="fa fa-bars"></i></a>
        </div>

        <div class="col-sm-2 logoWrap">
            <div class="swLogo" href="/" title="Episodes">herocast</div>
        </div>

        <nav class="col-sm-8">
            <%# links %>
            <a href="#/<% href %>" title="<% title %>">
                <%# text %><% text %><%/ text %>
                <%# icon %><i class="fa fa-<% icon %>"></i><%/ icon %>
            </a>
            <%/ links %>
        </nav>

        <div class="col-sm-1 playerToggle" ng-controller="PlayerController">
            <a id="toggleAudio" ng-click="toggleVisible()" ng-if="playerObj.status != 0">
                <i class="fa {{toggleStyle}}"></i>
            </a>
            <i class="fa fa-bars"></i>

        </div>
    </div>
    </div>
<%/ navBar %>
<!-- End Generated Code: mustache_templates/ui/navBar.mustache -->
